@model Car
@{
    ViewData["Title"] = "Edit Car";
}


<div>
    <h1>Edit a Car</h1>
    <hr />
</div>
<div class="row">
    <form asp-action="Update" asp-route-id="@Model.Id">
        <partial name="_form" model = "@Model"/>
        <button class="btn btn-primary" type="submit">Save</button>
        <a class="btn btn-secondary" asp-action="Cancel" asp-controller="Car">Cancel</a>
    </form>
</div>
